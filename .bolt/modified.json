[["/home/project/src/components/TranslationInterface.tsx","import React, { useState, useCallback } from 'react'\nimport { debounce } from 'lodash-es'\nimport { ArrowRight, Globe, ThumbsUp, ThumbsDown, Copy, Upload } from 'lucide-react'\n\nconst TranslationInterface: React.FC = () => {\n  const [inputText, setInputText] = useState('')\n  const [translations, setTranslations] = useState<Array<{ source: string; translated: string }>>([])\n  const [sourceLanguage] = useState('en')\n  const [targetLanguage] = useState('de')\n\n  const translateText = async (text: string) => {\n    // Mock translation (reverse the text)\n    const mockTranslation = text.split('').reverse().join('')\n    setTranslations(prev => [...prev, { source: text, translated: mockTranslation }])\n  }\n\n  const debouncedTranslate = useCallback(\n    debounce((text: string) => translateText(text), 300),\n    []\n  )\n\n  const handleInputChange = (e: React.ChangeEvent<HTMLInputElement>) => {\n    setInputText(e.target.value)\n    if (e.target.value) {\n      debouncedTranslate(e.target.value)\n    }\n  }\n\n  const handleSubmit = (e: React.FormEvent) => {\n    e.preventDefault()\n    if (inputText.trim()) {\n      translateText(inputText)\n      setInputText('')\n    }\n  }\n\n  return (\n    <div className=\"flex-1 flex flex-col max-w-7xl mx-auto w-full p-8 bg-neutral-50\">\n      <div className=\"flex-1 overflow-y-auto mb-4 space-y-0\">\n        {translations.map((translation, index) => (\n          <div key={index} className={`flex space-x-0 ${index % 2 === 0 ? 'bg-neutral-100' : ''}`}>\n            <div className=\"flex-1 bg-white border-r border-gray-100 p-4\">\n              <div className=\"whitespace-pre-wrap py-8\">{translation.source}</div>\n              <div className=\"flex items-center mb-2\">\n                \n                <div className=\"flex items-center\">\n                  <img src=\"https://i.pravatar.cc/24\" alt=\"User Avatar\" className=\"w-6 h-6 rounded-md mr-2\" />\n                  <span className=\"text-sm text-neutral-500 pr-3 font-normal\">English</span>\n                </div>\n                <button className=\"text-blue-600 px-1 py-1 bg-blue-100 rounded-md text-xs\">Translate</button>\n              </div>\n            </div>\n            <div className=\"flex-1 p-4\">\n              <div className=\"bg-neutral-0 border border-neutral-100 rounded-lg mr-4 p-4\">\n              <div className=\"whitespace-pre-wrap py-8\">{translation.translated}</div>\n              <div className=\"flex items-center justify-between mb-2\">\n                <div className=\"flex items-center\">\n                  <Upload className=\"w-6 h-6 text-gray-600 mr-2\" />\n                  <span className=\"text-sm font-medium\">Deutsch</span>\n                </div>\n                <div className=\"flex space-x-2\">\n                  <button className=\"p-1 hover:bg-gray-100 rounded-full\">\n                    <Copy className=\"w-4 h-4 text-gray-600\" />\n                  </button>\n                  <button className=\"p-1 hover:bg-gray-100 rounded-full\">\n                    <ThumbsUp className=\"w-4 h-4 text-gray-600\" />\n                  </button>\n                  <button className=\"p-1 hover:bg-gray-100 rounded-full\">\n                    <ThumbsDown className=\"w-4 h-4 text-gray-600\" />\n                  </button>\n                </div>\n              </div>\n             </div>\n            </div>\n          </div>\n        ))}\n      </div>\n      <form onSubmit={handleSubmit} className=\"relative\">\n        <input\n          type=\"text\"\n          value={inputText}\n          onChange={handleInputChange}\n          placeholder=\"Type something... or drag and drop / upload a supported file\"\n          className=\"w-full p-3 pr-24 border border-gray-300 rounded-full focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-transparent\"\n        />\n        <div className=\"absolute right-2 top-1/2 transform -translate-y-1/2 flex items-center\">\n          <button type=\"button\" className=\"p-2 hover:bg-gray-100 rounded-full mr-1\">\n            <Upload className=\"w-5 h-5 text-gray-600\" />\n          </button>\n          <button type=\"submit\" className=\"bg-blue-600 text-white p-2 rounded-full\">\n            <ArrowRight className=\"w-5 h-5\" />\n          </button>\n        </div>\n      </form>\n    </div>\n  )\n}\n\nexport default TranslationInterface"],["/home/project/src/components/Header.tsx","import React from 'react'\nimport { Menu, Plus } from 'lucide-react'\n\nconst Header: React.FC = () => {\n  return (\n    <header className=\"bg-white border-b border-gray-200 px-4 py-2 flex items-center justify-between\">\n      <div className=\"flex items-center\">\n        <button className=\"p-2 hover:bg-gray-100 rounded-full mr-2\">\n          <Menu className=\"w-6 h-6 text-gray-600\" />\n        </button>\n        <h1 className=\"text-md font-medium text-gray-800\">Spaces Inc. / Portal</h1>\n      </div>\n      <div className=\"flex items-center\">\n        <button className=\"bg-neutral-100 text-black px-3 py-2 rounded-lg text-sm font-medium flex items-center mr-4\">\n          <Plus className=\"w-4 h-4 mr-1\" /> New session\n        </button>\n        <img src=\"https://i.pravatar.cc/40\" alt=\"User Avatar\" className=\"w-8 h-8 rounded-lg\" />\n      </div>\n    </header>\n  )\n}\n\nexport default Header"]]